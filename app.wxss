/**app.wxss**/
.container {
   height: 100%;
   background: #fafafa;
   width: 100%;
   font-family: PingFangSC-Light,helvetica,'Heiti SC';
} 
.clearfix:after{
  content:".";
  display:block;
  height:0;
  clear:both;
  visibility:hidden;
}
view,image,text,navigator{
  box-sizing: border-box;
  padding:0;
  margin:0;
}
view,text{
  font-family: PingFangSC-Light,helvetica,'Heiti SC';
  font-size: 29rpx;
  color: #333;
}
